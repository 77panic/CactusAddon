{
  "schemaVersion": 1,
  "id": "cactus-addon",
  "version": "${version}",
  "name": "Cactus Addon",
  "description": "Cactus Addon",
  "authors": [
    "panic"
  ],
  "contact": {
    "repo": "https://github.com/MeteorDevelopment/meteor-addon-template"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "com.cactus.addon.AddonCactus"
    ]
  },
  "mixins": [
    "cactus.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25"
  },
  "depends": {
    "java": ">=21",
    "minecraft": [
      "${mc_version}"
    ],
    "meteor-client": "*"
  }
}
